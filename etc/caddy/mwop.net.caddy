testing.mwop.net {
    encode gzip
    header Strict-Transport-Security max-age=31536000;
    reverse_proxy localhost:9000 {
        header_down -Server
    }
    log {
        output file /var/log/caddy/testing.mwop.net {
            roll_keep 3
            roll_keep_for 30
        }
        format json
    }
}
